  "shortcuts": [
    {
      "name": "Random Animal",
      "short_name": "Random",
      "description": "Play a random animal animation",
      "url": "?action=random",
      "icons": [
        {
          "src": "/Animal-Funimation/android-icon-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Alpaca",
      "short_name": "Alpaca",
      "description": "Play alpaca animation",
      "url": "?animal=alpaca",
      "icons": [
        {
          "src": "/Animal-Funimation/android-icon-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Puppy",
      "short_name": "Puppy",
      "description": "Play puppy animation",
      "url": "?animal=puppy",
      "icons": [
        {
          "src": "/Animal-Funimation/android-icon-96x96.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "android",
      "url": "https://example.com/android-app",
      "id": "com.example.animalfunimation"
    }
  ],
  "file_handlers": [
    {
      "action": "?file=open",
      "accept": {
        "image/png": [".png"],
        "image/jpeg": [".jpg", ".jpeg"],
        "audio/wav": [".wav"]
      }
    }
  ],
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "share_target": {
    "action": "share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", "audio/*"]
        }
      ]
    }
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "note_taking": {
    "new_note_url": "?note=new"
  },
  "widgets": [
    {
      "name": "Animal Widget",
      "description": "Quick access to favorite animals",
      "tag": "animal-widget",
      "screenshots": [
        {
          "src": "/Animal-Funimation/screenshot1.png",
          "sizes": "320x240"
        }
      ],
      "icons": [
        {
          "src": "/Animal-Funimation/android-icon-192x192.png",
